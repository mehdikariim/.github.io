<script>
    function loadCSS(href){
        var ss = window.document.createElement('link'),
            ref = window.document.getElementsByTagName('head')[0];

        ss.rel = 'stylesheet';
        ss.href = href;
        ss.media = 'only x';
        ss.onload = function() {
            ss.media = 'all';
            window.postFoldCssLoaded = true;
            if (window.postFoldCssLoadedCallback) {
                window.postFoldCssLoadedCallback();
            }
        };

        ref.parentNode.insertBefore(ss, ref);
    }
    setTimeout( function() {
        loadCSS("https:\/\/d3j4c7e2o820k1.cloudfront.net\/css\/049809d-37bebaa.css");
    }, 0);
</script>
