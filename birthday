                        notifications: {
                            mobile: {
                                disable: true
                            }
                        }
                    }
                }
            };

            chatSuppressInTitle(chatSuppressed);

            if (window.zE) {
                zE('webWidget', 'hide');
                zE('webWidget:on', 'chat:status', function(status) {
                    if ('offline' === status || chatSuppressed) {

                        zE('webWidget', 'updateSettings', {
                            webWidget: {
                                contactForm: {
                                    suppress: false
                                },
                                helpCenter: {
                                    suppress: false
                                }
                            }
                        });

                        zE('webWidget', 'helpCenter:setSuggestions', { labels: ['landing'] });

                        toggleChatOnly(false);

                    } else {

                        zE('webWidget', 'updateSettings', {
                            webWidget: {
                                helpCenter: {
                                    suppress: true
                                },
                                contactForm: {
                                    suppress: true
                                }
                            }
                        });

                        toggleChatOnly(true);
                    }

                    chatSuppressInTitle(chatSuppressed);
                });

                zE(function() {
                    zE.setLocale("en-US");
                    zE('webWidget', 'updateSettings', window.zESettings);
                });
            }
        }

        function toggleChatOnly(show) {
            if (!chatButton || !helpButton) {
                return;
            }
            if (show) {
                helpButton.classList.remove('shown');
                chatButton.classList.add('shown');
            } else {
                helpButton.classList.add('shown');
                chatButton.classList.remove('shown');
            }
        }

        function setUpButtons() {
            if (!chatButton || !helpButton) {
                return;
            }
            helpButton.addEventListener('click', function(event) {
                event.preventDefault();
                if(window.zE){
                    zE(function() {
                        zE.activate();
                    }); return false;
                }
            });
            chatButton.addEventListener('click', function(event) {
                event.preventDefault();
                if(window.zE){
                    zE(function() {
                        zE.activate();
                    }); return false;
                }
            });
        }

        function chatSuppressInTitle(suppress) {
            if (suppress) {
                document.title = document.title.replace(' ★', '');
                document.title = document.title + ' ★';
            } else {
                document.title = document.title.replace(' ★', '');
            }
        }

        var script = document.createElement('script');
        script.type = 'text/javascript';
        script.id = "ze-snippet";
        script.defer = true;
        script.async = true;
        script.src = 'https://static.zdassets.com/ekr/snippet.js?key=996b5a36-276d-4554-958f-4bdc24b2ae19';
        script.onload = function() {
            var hideEnd = dataLayer && dataLayer.hide && dataLayer.hide.end;
            if (hideEnd) {
                var timeout;
                dataLayer.hide.end = function() {
                    window.clearTimeout(timeout);
                    initZendesk();
                    hideEnd();
                };
                timeout = window.setTimeout(dataLayer.hide.end, 4000);
            } else {
                initZendesk();
            }

            setUpButtons();
        };

        document.body.appendChild(script);

    }());
</script>

<script>
    window.addEventListener('load', function() {
       // Prefetching the designer when everything else is loaded.

        function createLink(rel, url) {
            var link = window.document.createElement('link'),
                head = window.document.getElementsByTagName('head')[0];

            link.rel = rel;
            link.href = url;
            head.parentNode.insertBefore(link, head);
        }

        createLink('prefetch', "\/en\/designer");
        createLink('next', "\/en\/designer");
        createLink('prerender', "\/en\/designer");

                createLink('prefetch', "https:\/\/d3j4c7e2o820k1.cloudfront.net\/css\/3660b9f-a3b718e.css");
                        createLink('prefetch', "https:\/\/d3j4c7e2o820k1.cloudfront.net\/js\/155db98-0faab4d.js");
            });
</script>

</body>
</html>
